<!-- viaje BPEL Process [Generated by the Eclipse BPEL Designer] -->
<!-- Date: Mon Dec 26 20:22:03 CET 2016 -->
<bpel:process name="viaje"
         targetNamespace="http://viaje"
         suppressJoinFailure="yes"
         xmlns:tns="http://viaje"
         xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         xmlns:emp="http://empleado">

    <!-- Import the client WSDL -->
    <bpel:import namespace="http://empleado" location="empleadoArtifacts.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"></bpel:import>
    <bpel:import location="viajeArtifacts.wsdl" namespace="http://viaje" 
	        importType="http://schemas.xmlsoap.org/wsdl/" />
         
    <!-- ================================================================= -->         
    <!-- PARTNERLINKS                                                      -->
    <!-- List of services participating in this BPEL process               -->
    <!-- ================================================================= -->         
    <bpel:partnerLinks>
        <!-- The 'client' role represents the requester of this service. -->
        <bpel:partnerLink name="client"
                     partnerLinkType="tns:viaje"
                     myRole="viajeProvider"
                     />
        <bpel:partnerLink name="empleado" partnerLinkType="emp:empleado" partnerRole="empleadoProvider"></bpel:partnerLink>
    </bpel:partnerLinks>
  
    <!-- ================================================================= -->         
    <!-- VARIABLES                                                         -->
    <!-- List of messages and XML documents used within this BPEL process  -->
    <!-- ================================================================= -->         
    <bpel:variables>
        <!-- Reference to the message passed as input during initiation -->
        <bpel:variable name="input"
                  messageType="tns:viajeRequestMessage"/>
                  
        <!-- 
          Reference to the message that will be returned to the requester
          -->
        <bpel:variable name="output"
                  messageType="tns:viajeResponseMessage"/>
        
        
        
        <bpel:variable name="empleadoResponse" messageType="emp:empleadoResponseMessage"></bpel:variable>
        <bpel:variable name="empleadoRequest" messageType="emp:empleadoRequestMessage"></bpel:variable>
    </bpel:variables>

    <!-- ================================================================= -->         
    <!-- ORCHESTRATION LOGIC                                               -->
    <!-- Set of activities coordinating the flow of messages across the    -->
    <!-- services integrated within this business process                  -->
    <!-- ================================================================= -->         
    <bpel:sequence name="main">
        
        <!-- Receive input from requester. 
             Note: This maps to operation defined in viaje.wsdl 
             -->
        <bpel:receive name="receiveInput" partnerLink="client"
                 portType="tns:viaje"
                 operation="process" variable="input"
                 createInstance="yes"/>
        
        <!-- Generate reply to synchronous request -->
        <bpel:assign validate="no" name="fromInputToEmpleado">
            
            
            <bpel:copy>
                <bpel:from><bpel:literal><tns:empleadoRequest xmlns:tns="http://empleado" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <tns:nombre>tns:nombre</tns:nombre>
</tns:empleadoRequest>
</bpel:literal></bpel:from>
                <bpel:to variable="empleadoRequest" part="payload"></bpel:to>
            </bpel:copy>
            <bpel:copy>
                <bpel:from part="payload" variable="input">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[tns:nombre]]></bpel:query>
                </bpel:from>
                <bpel:to part="payload" variable="empleadoRequest">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[emp:nombre]]></bpel:query>
                </bpel:to>
            </bpel:copy>
        </bpel:assign>
        <bpel:invoke name="empleado" partnerLink="empleado" operation="getTipo" portType="emp:empleado" inputVariable="empleadoRequest" outputVariable="empleadoResponse">
        </bpel:invoke>
        <bpel:assign validate="no" name="fromEmpleadoToOutput">
            
            
            <bpel:copy>
                <bpel:from><bpel:literal><tns:viajeResponse xmlns:tns="http://viaje" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <tns:tipo>tns:tipo</tns:tipo>
</tns:viajeResponse>
</bpel:literal></bpel:from>
                <bpel:to variable="output" part="payload"></bpel:to>
            </bpel:copy>
            <bpel:copy>
                <bpel:from part="payload" variable="empleadoResponse">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[emp:tipo]]></bpel:query>
                </bpel:from>
                <bpel:to part="payload" variable="output">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[tns:tipo]]></bpel:query>
                </bpel:to>
            </bpel:copy>
        </bpel:assign>
        <bpel:reply name="replyOutput" 
               partnerLink="client"
               portType="tns:viaje"
               operation="process" 
               variable="output"
               />
    </bpel:sequence>
</bpel:process>

